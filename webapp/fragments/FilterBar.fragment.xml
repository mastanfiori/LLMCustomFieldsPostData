					<!-- <smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration7" key="FollowUpDate" index="8" label="{i18n>FollowUpDate}" groupId="_BASIC"
						width="300px" mandatory="notMandatory" visibleInAdvancedArea="true"/> -->
						<smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration7" key="FollowUpDate" index="8" label="{i18n>FollowUpDate}" groupId="_BASIC"
						visibleInAdvancedArea="true"
							conditionType="{
								module: 'sap.ui.comp.config.condition.DateRangeType',
								ignoreTime: false,
								operations: {
									filter: [
										{path: 'category', contains: 'MONTH', exclude:true},
										{path: 'category', contains: 'QUARTER', exclude:true},
										{path: 'key', equals: 'TO', exclude:true}
									]
								}
							}"/>
					<smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration8" key="Plant" index="9" label="{i18n>Plant}" groupId="_BASIC"
						width="300px" mandatory="notMandatory" visibleInAdvancedArea="true"></smartFilterBar:ControlConfiguration>
					<!-- <smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration9" key="ConfControl" index="10" label="{i18n>ConfirmationControlKey}" groupId="_BASIC"
						width="300px" mandatory="notMandatory" visibleInAdvancedArea="true"/> -->
					<smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration10" key="POStatus" index="11" label="{i18n>Status}" groupId="_BASIC"
						width="300px" mandatory="notMandatory" visibleInAdvancedArea="true"/>
				
                    </smartFilterBar:controlConfiguration>
