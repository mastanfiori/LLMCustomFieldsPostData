						width="300px" mandatory="notMandatory" visibleInAdvancedArea="true"></smartFilterBar:ControlConfiguration>
					<smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration1" key="PurchaseOrder" index="2" label="{i18n>PurchaseOrder}" groupId="_BASIC"
						width="300px" mandatory="notMandatory" visibleInAdvancedArea="true"></smartFilterBar:ControlConfiguration>
					<smartFilterBar:ControlConfiguration id="_IDEGen_controlconfiguration2" key="PurchasingGroup" index="3" label="{i18n>PurchasingGroup}" groupId="_BASIC"
